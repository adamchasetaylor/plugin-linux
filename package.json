{
  "name": "plugin-linux",
  "description": "linux plugin for cli training",
  "author": "Adam Taylor, Gareth Paul Jones",
  "bugs": "https://github.com/twilio/twilio-cli/issues",
  "dependencies": {
    "@nexssp/os": "^1.0.24",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.15.1",
    "@twilio/cli-core": "^5.0.0",
    "chalk": "^4.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@twilio/cli-test": "^2.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-twilio": "^1.31.0",
    "globby": "^11.0.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.2.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/src"
  ],
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "name": "linux",
    "commands": "./src/commands",
    "bin": "twilio",
    "repositoryPrefix": "<%- repo %>/blob/<%- version %>/<%- commandPath %>",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "linux": {
        "description": "Commands for Linux Distros"
      },
      "linux:headless": {
        "description": "Commands for Headless Environments"
      }
    }
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint --ignore-path .gitignore . && npm audit",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc --check-coverage --lines 90 --reporter=html --reporter=text mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "pretty": "prettier --write \"./**/*.{html,js,jsx,json}\""
  }
}
